{
  "name": "doc-hema-team-extension",
  "version": "4.0.0",
  "description": "BIN Autofiller & Open Links - Doc_HEMA Team Extension with Subscription System",
  "main": "background.js",
  "scripts": {
    "build": "echo 'Extension built successfully'",
    "test": "echo 'No tests specified'",
    "dev": "echo 'Development mode - load extension in Chrome'"
  },
  "keywords": [
    "chrome-extension",
    "bin-autofiller",
    "card-filler",
    "subscription-system",
    "doc-hema-team"
  ],
  "author": "Doc_HEMA Team",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/doc-hema-team/extension.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {},
  "dependencies": {},
  "manifest": {
    "version": 3,
    "permissions": [
      "activeTab",
      "storage",
      "scripting",
      "tabs",
      "contextMenus",
      "commands"
    ],
    "host_permissions": [
      "http://localhost:3000/*",
      "https://*/*",
      "http://*/*"
    ]
  },
  "files": [
    "manifest.json",
    "popup.html",
    "popup.js",
    "background.js",
    "content.js",
    "settings.html",
    "settings.js",
    "README.md",
    "icon16.png",
    "icon48.png",
    "icon128.png"
  ]
} 